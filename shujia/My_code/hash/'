#include<iostream>
#include<bits/stl_function.h>
#include<hash_fun.h>
#include<ext/functional>
using namespace std;
#include<hashtable.h>

int  main()
{
	__gnu_cxx::hashtable<int, int, __gnu_cxx::hash<int>, __gnu_cxx::identity<int>, __gnu_cxx::equal_to<int>  >  ht(50,hash<int>(),equal_to<int>());
	return 0;
}


